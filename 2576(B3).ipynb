      "source": [
        "N = list(map(int, [input() for _ in range(7)]))\n",
        "\n",
        "sum = 0\n",
        "odd = []\n",
        "for i in N:\n",
        "    if i%2 != 0:\n",
        "        sum += i\n",
        "        odd.append(i)\n",
        "    else:\n",
        "        continue\n",
        "\n",
        "if len(odd) == 0:\n",
        "    print(-1)\n",
        "else:\n",
        "    print(sum, min(odd), sep='\\n')"
      ],
